
{{#each randomBeer}}

    <section id="random-container">

        {{> beerpartial this}}

        <h3>Food Pairing</h3>
            <ul>
                {{#each this.food_pairing}}
                <li>{{this}}</li>
                {{/each}}
             </ul>
        <p>{{this.brewers_tips}}</p>
     </section>
    
{{/each}}